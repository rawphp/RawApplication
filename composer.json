{
    "name": "rawphp/raw-application",
    "type": "library",
    "description": "RawApplication is a core application class that is a central point for RawPHP framework and other applications.",
    "keywords": [
        "application",
        "app",
        "core"
    ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "rawphp/raw-support": "dev-master",
        "rawphp/raw-container": "dev-master",
        "rawphp/raw-request": "0.*@dev",
        "rawphp/raw-session": "0.*@dev",
        "rawphp/raw-router": "0.*@dev",
        "rawphp/raw-log": "0.*@dev",
        "rawphp/raw-mail": "0.*@dev",
        "rawphp/raw-error-handler": "0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawApplication\\": "src/RawPHP/RawApplication/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev"
}